{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "plugins":["node_modules/jsdoc-inheritance-diagram"],
    "source": {
      "exclude": [
        ".git",
        "node_modules",
        "plugins",
        "WebDriverAgent",
        "Documentation",
        "chromedriver",
        "ffprofile",
        "man",
        "mobiledevice",
        "sample_project",
        "support",
        "test",
        ".docx",
        "SodaVisualEditor/static/docs",
        "SodaVisualEditor/static/external"
      ]
    },
    "opts": {
        "type": "flowdiagram",
        "vizdataname": "vizdata",
        "recurse": true,
        "destination": "./SodaVisualEditor/static/docs/",
        "readme": "./Readme.md",
        "inheritance-diagram": {
          "externalLinks": {
              "SuperRoot": "https://super/root/url",
              "ExtClass": "http://link.to/external/class/documentation.html"
          },
          "css": ".parent rect {fill: lightgray;}",
          "node": {
              "dimensions": {
                  "width": 250
              }
          }
        }
    },
    "templates": {
        "applicationName": "Simple Object Driven Automation",
        "linenums": true,
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        },
        "systemName"      : "DocStrap",
        "footer"          : "",
        "copyright"       : "DocStrap Copyright Â© 2012-2013 The contributors to the JSDoc3 and DocStrap projects.",
        "navType"         : "vertical",
        "theme"           : "cerulean",
        "linenums"        : true,
        "collapseSymbols" : false,
        "inverseNav"      : true
    }
}
